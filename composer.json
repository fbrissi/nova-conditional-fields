{
    "name": "firework/nova-conditional-fields",
    "description": "A component for grouping fields that depend on other field values.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Firework\\NovaConditionalFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firework\\NovaConditionalFields\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
